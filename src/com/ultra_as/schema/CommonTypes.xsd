						<xs:element name="IrregularityDetails" type="IrregularityDetails" nillable="true" minOccurs="0" maxOccurs="2"/>
						<xs:element name="IsGeneralAviationFlight" type="IsGeneralAviationFlight" nillable="true" minOccurs="0"/>
						<xs:element name="IsReturnFlight" type="IsReturnFlight" nillable="true" minOccurs="0"/>
						<xs:element name="IsTransferFlight" type="IsTransferFlight" nillable="true" minOccurs="0"/>
						<xs:element name="HasVIPs" type="HasVIPs" nillable="true" minOccurs="0"/>
						<xs:element name="LinkedFlight" type="LinkedFlight" nillable="true" minOccurs="0"/>
						<xs:element name="MasterFlight" type="MasterFlight" nillable="true" minOccurs="0"/>
						<xs:element name="NewFlightReason" type="NewFlightReason" nillable="true" minOccurs="0"/>
						<xs:element name="OperationType" type="OperationType" nillable="true" minOccurs="0" maxOccurs="2"/>
						<xs:element name="ShareFlight" type="ShareFlight" nillable="true" minOccurs="0" maxOccurs="6"/>
						<xs:element name="TransferFlight" type="TransferFlight" minOccurs="0" maxOccurs="20"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Load" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Elements relating to aircraft payload such as number of passengers, crew, weight of baggage/cargo/mail.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence minOccurs="0">
						<xs:element name="Cargo" nillable="true" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Elements relating to aircraft baggage, freight and mail.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="TotalBaggageCount" type="TotalBaggageCount" nillable="true" minOccurs="0"/>
									<xs:element name="TotalBaggageWeight" type="TotalBaggageWeight" nillable="true" minOccurs="0"/>
									<xs:element name="TotalFreightWeight" type="TotalFreightWeight" nillable="true" minOccurs="0"/>
									<xs:element name="TotalLoadWeight" type="TotalLoadWeight" nillable="true" minOccurs="0"/>
									<xs:element name="TotalMailWeight" type="TotalMailWeight" nillable="true" minOccurs="0"/>
									<xs:element name="TransferBaggageCount" type="TransferBaggageCount" nillable="true" minOccurs="0"/>
									<xs:element name="TransferBaggageWeight" type="TransferBaggageWeight" nillable="true" minOccurs="0"/>
									<xs:element name="TransferCargoWeight" type="TransferCargoWeight" nillable="true" minOccurs="0"/>
									<xs:element name="TransferMailWeight" type="TransferMailWeight" nillable="true" minOccurs="0"/>
									<xs:element name="TransitBaggageCount" type="TransitBaggageCount" nillable="true" minOccurs="0"/>
									<xs:element name="TransitBaggageWeight" type="TransitBaggageWeight" nillable="true" minOccurs="0"/>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="PassengerCounts" nillable="true" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Elements relating to passenger figures.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="AdultPassengerCount" type="AdultPassengerCount" nillable="true" minOccurs="0"/>
									<xs:element name="BusinessClassPassengerCount" type="BusinessClassPassengerCount" nillable="true" minOccurs="0"/>
									<xs:element name="ChildPassengerCount" type="ChildPassengerCount" nillable="true" minOccurs="0"/>
									<xs:element name="DomesticPassengerCount" type="DomesticPassengerCount" nillable="true" minOccurs="0"/>
									<xs:element name="EconomyClassPassengerCount" type="EconomyClassPassengerCount" nillable="true" minOccurs="0"/>
									<xs:element name="FirstClassPassengerCount" type="FirstClassPassengerCount" nillable="true" minOccurs="0"/>
									<xs:element name="InfantPassengerCount" type="InfantPassengerCount" nillable="true" minOccurs="0"/>
									<xs:element name="InternationalPassengerCount" type="InternationalPassengerCount" nillable="true" minOccurs="0"/>
									<xs:element name="LocalPassengerCount" type="LocalPassengerCount" nillable="true" minOccurs="0"/>
									<xs:element name="NonTransferPassengerCount" type="NonTransferPassengerCount" nillable="true" minOccurs="0"/>
									<xs:element name="TotalPassengerCount" type="TotalPassengerCount" nillable="true" minOccurs="0"/>
									<xs:element name="TransferPassengerCount" type="TransferPassengerCount" nillable="true" minOccurs="0"/>
									<xs:element name="TransitPassengerCount" type="TransitPassengerCount" nillable="true" minOccurs="0"/>
									<xs:element name="WheelchairPassengerCount" type="WheelchairPassengerCount" nillable="true" minOccurs="0"/>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="TotalCrewCount" type="TotalCrewCount" nillable="true" minOccurs="0"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="OperationalTimes" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Elements relating to operational flight times.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence minOccurs="0">
						<xs:element name="ActualOffBlocksDateTime" type="ActualOffBlocksDateTime" nillable="true" minOccurs="0"/>
						<xs:element name="ActualOnBlocksDateTime" type="ActualOnBlocksDateTime" nillable="true" minOccurs="0"/>
						<xs:element name="EstimatedDateTime" type="EstimatedDateTime" nillable="true" minOccurs="0"/>
						<xs:element name="EstimatedFlightDuration" type="EstimatedFlightDuration" nillable="true" minOccurs="0"/>
						<xs:element name="FinalApproachDateTime" type="FinalApproachDateTime" nillable="true" minOccurs="0"/>
						<xs:element name="LatestKnownDateTime" type="LatestKnownDateTime" nillable="true" minOccurs="0"/>
						<xs:element name="LatestKnownDateTimeSource" type="LatestKnownDateTimeSource" nillable="true" minOccurs="0"/>
						<xs:element name="NextInformationDateTime" type="NextInformationDateTime" nillable="true" minOccurs="0"/>
						<xs:element name="NextStationActualDateTime" type="NextStationActualDateTime" nillable="true" minOccurs="0"/>
						<xs:element name="NextStationEstimatedDateTime" type="NextStationEstimatedDateTime" nillable="true" minOccurs="0"/>
						<xs:element name="NextStationScheduledDateTime" type="NextStationScheduledDateTime" nillable="true" minOccurs="0"/>
						<xs:element name="PreviousStationAirborneDateTime" type="PreviousStationAirborneDateTime" nillable="true" minOccurs="0"/>
						<xs:element name="PreviousStationEstimatedDateTime" type="PreviousStationEstimatedDateTime" nillable="true" minOccurs="0"/>
						<xs:element name="PreviousStationScheduledDateTime" type="PreviousStationScheduledDateTime" nillable="true" minOccurs="0"/>
						<xs:element name="ScheduledDateTime" type="ScheduledDateTime" nillable="false" minOccurs="0"/>
						<xs:element name="TenMileOutDateTime" type="TenMileOutDateTime" nillable="true" minOccurs="0"/>
						<xs:element name="WheelsDownDateTime" type="WheelsDownDateTime" nillable="true" minOccurs="0"/>
						<xs:element name="WheelsUpDateTime" type="WheelsUpDateTime" nillable="true" minOccurs="0"/>
						<xs:element name="FirstStationScheduledDateTime" type="FirstStationScheduledDateTime" nillable="true" minOccurs="0"/>
						<xs:element name="SecondStationScheduledDateTime" type="SecondStationScheduledDateTime" nillable="true" minOccurs="0"/>
						<xs:element name="ThirdStationScheduledDateTime" type="ThirdStationScheduledDateTime" nillable="true" minOccurs="0"/>
						<xs:element name="FourthStationScheduledDateTime" type="FourthStationScheduledDateTime" nillable="true" minOccurs="0"/>
						<xs:element name="FifthStationScheduledDateTime" type="FifthStationScheduledDateTime" nillable="true" minOccurs="0"/>
						<xs:element name="SixthStationScheduledDateTime" type="SixthStationScheduledDateTime" nillable="true" minOccurs="0"/>
						<xs:element name="OriginStationScheduledDateTime" type="OriginStationScheduledDateTime" nillable="true" minOccurs="0"/>
						<xs:element name="DestinationStationScheduledDateTime" type="DestinationStationScheduledDateTime" nillable="true" minOccurs="0"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="PortsOfCall" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>List of stops on the flight itinerary presented in 'flying' order i.e. sequence in which aircraft visits them.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="PortOfCallIATACode" type="PortOfCallIATACode" nillable="true" maxOccurs="7"/>
						<xs:element name="PortOfCallICAOCode" type="PortOfCallICAOCode" nillable="true" minOccurs="0" maxOccurs="7"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Public" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Elements to be used when generating public facing information displays e.g. FIDS, teletext, World Wide Web etc.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence minOccurs="0">
						<xs:element name="FlightIdentity" type="FlightIdentity" nillable="true" minOccurs="0"/>
						<xs:element name="PublicCarrierCode" type="PublicCarrierCode" nillable="true" minOccurs="0"/>
						<xs:element name="PublicDateTime" type="PublicDateTime" nillable="true" minOccurs="0"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="FlightDirection">
		<xs:annotation>
			<xs:documentation>Direction of flight (Arrival, Departure).</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Arrival"/>
			<xs:enumeration value="Departure"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="FlightElapsedJetTime">
		<xs:annotation>
			<xs:documentation>Jet powered journey time from an airport </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:duration"/>
	</xs:simpleType>
	<xs:simpleType name="FlightElapsedPropellerTime">
		<xs:annotation>
			<xs:documentation>Propeller powered journey time from an airport.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:duration"/>
	</xs:simpleType>
	<xs:complexType name="FlightIdentification">
		<xs:annotation>
			<xs:documentation>Unique identification of a flight is determined by flight identity, flight direction, flight repeat count and scheduled date. For scheduled flights, the identification can be supplemented by an IATA flight identifier and/or an ICAO flight identifier. The flight repeat count is used to determine flights that operate more than one time on a single day.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="FlightIdentity" type="FlightIdentity" nillable="false"/>
			<xs:element name="FlightDirection" type="FlightDirection" nillable="false"/>
			<xs:element name="FlightRepeatCount" type="FlightRepeatCount" nillable="true" minOccurs="0"/>
			<xs:element name="ScheduledDate" type="ScheduledDate" nillable="false"/>
			<xs:element name="IATAFlightIdentifier" type="IATAFlightIdentifier" nillable="false" minOccurs="0"/>
			<xs:element name="ICAOFlightIdentifier" type="ICAOFlightIdentifier" nillable="false" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="FlightFilterData">
		<xs:annotation>
			<xs:documentation>The old and new set of data the IMB requires to identify flight filter changes.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="AirportIATACode" nillable="true" minOccurs="0">
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="LocationIATACode">
							<xs:attribute name="previousValue" type="PreviousValue" use="optional"/>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="CarrierIATACode" nillable="true" minOccurs="0">
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="CarrierIATACode">
							<xs:attribute name="previousValue" type="PreviousValue" use="optional"/>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="CarrierICAOCode" nillable="true" minOccurs="0">
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="CarrierICAOCode">
							<xs:attribute name="previousValue" type="PreviousValue" use="optional"/>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="CodeShareStatus" nillable="true" minOccurs="0">
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="CodeShareStatus">
							<xs:attribute name="previousValue" type="PreviousValue" use="optional"/>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="FlightNatureCode" nillable="true" minOccurs="0">
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="FlightNatureCode">
							<xs:attribute name="previousValue" type="PreviousValue" use="optional"/>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="FlightSectorCode" nillable="true" minOccurs="0">
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="FlightSectorCode">
							<xs:attribute name="previousValue" type="PreviousValue" use="optional"/>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="FlightServiceTypeIATACode" nillable="true" minOccurs="0">
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="FlightServiceTypeIATACode">
							<xs:attribute name="previousValue" type="PreviousValue" use="optional"/>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="FlightType" nillable="true" minOccurs="0">
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="FlightType">
							<xs:attribute name="previousValue" type="PreviousValue" use="optional"/>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="TerminalCode" nillable="true" minOccurs="0">
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="TerminalCode">
							<xs:attribute name="previousValue" type="PreviousValue" use="optional"/>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="FlightFirstDateTime">
		<xs:annotation>
			<xs:documentation>Start of validity date for this flight's operations (includes the flight's scheduled time).</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:dateTime"/>
	</xs:simpleType>
	<xs:simpleType name="FlightFrequency">
		<xs:annotation>
			<xs:documentation>Interval of operation (e.g.1 = weekly, 2 = fortnightly, 3 = every three weeks, etc.)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer">
			<xs:minInclusive value="1"/>
			<xs:maxInclusive value="5"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="FlightID">
		<xs:annotation>
			<xs:documentation>Identification of the flight record. Note: a long data type is used to ensure that XML generation tools cater for very large integer values.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:long">
			<xs:maxInclusive value="999999999999999999"/>
			<xs:minInclusive value="0"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="FlightIdentity">
		<xs:annotation>
			<xs:documentation>Identity of the flight. This is typically comprised of flight carrier, flight number and an optional suffix. For general aviation flights the format is free text.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="8"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="FlightLastDateTime">
		<xs:annotation>
			<xs:documentation>End of validity date for this flight's operations (includes the flight's scheduled time).</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:dateTime"/>
	</xs:simpleType>
	<xs:simpleType name="FlightNatureCode">
		<xs:annotation>
			<xs:documentation>The code defining the nature of the flight. For example, passenger, cargo, training.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="3"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="FlightNatureDescription">
		<xs:annotation>
			<xs:documentation>Description or name of flight nature.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="30"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="FlightNumber">
		<xs:annotation>
			<xs:documentation>The number that is used to identify a flight - forms part of the flight identity. The number is zero padded to three digits (e.g. '73' will become '073').</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="4"/>
			<xs:minLength value="1"/>
			<xs:pattern value="[0-9]{1,4}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="FlightOperatesOvernight">
		<xs:annotation>
			<xs:documentation>Indicates whether the flight's corresponding flight operates on a different day.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:boolean">
			<xs:pattern value="false|true"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="FlightRepeatCount">
		<xs:annotation>
			<xs:documentation>Flight repeat count, will normally be 0 but can be 1-99 for repeated arrival or departures.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer">
			<xs:minInclusive value="0"/>
			<xs:maxInclusive value="99"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="FlightSeason">
		<xs:annotation>
			<xs:documentation>Season that the flight operates in.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="10"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="FlightSectorCode">
		<xs:annotation>
			<xs:documentation>The sector within which the flight operates (I-International, D-Domestic, M-Mixed, R-Regional).</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="D"/>
			<xs:enumeration value="I"/>
			<xs:enumeration value="M"/>
			<xs:enumeration value="R"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="FlightSectorDescription">
		<xs:annotation>
			<xs:documentation>Description or name of flight sector.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="30"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="FlightServiceTypeDescription">
		<xs:annotation>
			<xs:documentation>Description of the flight service type.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="30"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="FlightServiceTypeIATACode">
		<xs:annotation>
			<xs:documentation>IATA assigned code for type of flight service. For example, J = Normal Passenger Service, M = Mail Only.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="1"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="FlightShareStatus">
		<xs:annotation>
			<xs:documentation>Selected flight share status (master flights or all flights) - used to filter messages.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="MasterFlightsOnly"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="FlightStatusCode">
		<xs:annotation>
			<xs:documentation>The status of the flight during its progression (AB-Airborne, BD-Boarding, CX-Cancelled, DV-Diverted, ES-Estimated, EX-Expected, FB-First Bag, FS-Final Approach, FX-Flight Fixed, GA-Gate Attended, GC-Gate Close, GO-Gate Open, LB-Last Bag, LC-Last Call, LD-Landed, NI-Next Information, OB-On/Off Blocks, OT-On Time, OV-Overshoot, RS-Return To Stand, SH-Scheduled, ZN-Zoning).</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:simpleType name="FlightSuffix">
		<xs:annotation>
			<xs:documentation>Optional single character that can be appended to some flight identities. Some suffices may have specific meanings, for example C=charter flight, P=positioning flight, T=training flight, Y=Delayed from yesterday. However, these meanings are not standard across all systems so the IATA service type element should be used in preference.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="1"/>
			<xs:enumeration value="A"/>
			<xs:enumeration value="B"/>
			<xs:enumeration value="C"/>
			<xs:enumeration value="D"/>
			<xs:enumeration value="E"/>
			<xs:enumeration value="F"/>
			<xs:enumeration value="G"/>
			<xs:enumeration value="H"/>
			<xs:enumeration value="I"/>
			<xs:enumeration value="J"/>
			<xs:enumeration value="K"/>
			<xs:enumeration value="L"/>
			<xs:enumeration value="M"/>
			<xs:enumeration value="N"/>
			<xs:enumeration value="O"/>
			<xs:enumeration value="P"/>
			<xs:enumeration value="Q"/>
			<xs:enumeration value="R"/>
			<xs:enumeration value="S"/>
			<xs:enumeration value="T"/>
			<xs:enumeration value="U"/>
			<xs:enumeration value="V"/>
			<xs:enumeration value="W"/>
			<xs:enumeration value="X"/>
			<xs:enumeration value="Y"/>
			<xs:enumeration value="Z"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="FlightTransitCode">
		<xs:annotation>
			<xs:documentation>If set, indicates that the flight is a transit flight (Y-Normal Transit Flight, T-Technical Transit Flight)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="T"/>
			<xs:enumeration value="Y"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="FlightType">
		<xs:annotation>
			<xs:documentation>Selected type code (GA flights or scheduled flights) - used to filter messages.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="GeneralAviation"/>
			<xs:enumeration value="Scheduled"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="FlightWindow">
		<xs:annotation>
			<xs:documentation>The active flight window that can be requested by a client (D-1 to D+7, ShortTerm, LongTerm, All).</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="D0"/>
			<xs:enumeration value="D+1"/>
			<xs:enumeration value="D+2"/>
			<xs:enumeration value="D+3"/>
			<xs:enumeration value="D+4"/>
			<xs:enumeration value="D+5"/>
			<xs:enumeration value="D+6"/>
			<xs:enumeration value="D+7"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="FreightCapacity">
		<xs:annotation>
			<xs:documentation>Capacity of freight that an aircraft can accommodate.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer">
			<xs:minInclusive value="0"/>
			<xs:maxInclusive value="999999"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="Gate">
		<xs:annotation>
			<xs:documentation>Full description of a flight's gate data.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="GateBoardingStatus" type="GateBoardingStatus" nillable="true" minOccurs="0"/>
			<xs:element name="GateCloseDateTime" type="GateCloseDateTime" nillable="true" minOccurs="0"/>
			<xs:element name="GateNumber" type="GateNumber" nillable="true"/>
			<xs:element name="GateOpenDateTime" type="GateOpenDateTime" nillable="true" minOccurs="0"/>
			<xs:element name="GateRole" type="GateRole" nillable="false"/>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="GateBoardingStatus">
		<xs:annotation>
			<xs:documentation>Status of boarding process within a departure gate, which provides additional detail to the main FlightStatus (Open, Attended, Boarding, Last Call, Close).</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="A"/>
			<xs:enumeration value="B"/>
			<xs:enumeration value="C"/>
			<xs:enumeration value="L"/>
			<xs:enumeration value="O"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="GateCloseDateTime">
		<xs:annotation>
			<xs:documentation>Date and time of gate closure for a departing flight.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:dateTime"/>
	</xs:simpleType>
	<xs:simpleType name="GateDescription">
		<xs:annotation>
			<xs:documentation>Description of the gate.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="30"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="GateLastCallDateTime">
		<xs:annotation>
			<xs:documentation>Time and date when the gate's last call status was raised.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:dateTime"/>
	</xs:simpleType>
	<xs:simpleType name="GateNumber">
		<xs:annotation>
			<xs:documentation>Gate identity.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="8"/>
			<xs:minLength value="1"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="GateOpenDateTime">
		<xs:annotation>
			<xs:documentation>Date and time at which departure gate first opened for a departing flight.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:dateTime"/>
	</xs:simpleType>
	<xs:simpleType name="GateRole">
		<xs:annotation>
			<xs:documentation>Gate position role - describes the purpose of each position when more than one is allocated to a single flight. Note: Current limit is two gates per flight</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Primary"/>
			<xs:enumeration value="Secondary"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="GateStatus">
		<xs:annotation>
			<xs:documentation>Operational status of the gate (Attended, Open, Boarding, Close, Last Call, Serviceable, Unserviceable).</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="A"/>
			<xs:enumeration value="B"/>
			<xs:enumeration value="C"/>
			<xs:enumeration value="L"/>
			<xs:enumeration value="O"/>
			<xs:enumeration value="S"/>
			<xs:enumeration value="U"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="GateStatusEndDateTime">
		<xs:annotation>
			<xs:documentation>The gate's status end period.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:dateTime"/>
	</xs:simpleType>
	<xs:simpleType name="GateStatusReason">
		<xs:annotation>
			<xs:documentation>Free text providing additional information regarding the status of the resource.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="AbstractTypeStatusReason"/>
	</xs:simpleType>
	<xs:simpleType name="GateStatusStartDateTime">
		<xs:annotation>
			<xs:documentation>The gate's status start period.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:dateTime"/>
	</xs:simpleType>
	<xs:complexType name="HandlingAgent">
		<xs:annotation>
			<xs:documentation>Description of an individual agent and the specific handling services they provide for a given flight.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="HandlingAgentIATACode" type="HandlingAgentIATACode" nillable="true"/>
			<xs:element name="HandlingAgentService" type="HandlingAgentService" nillable="false"/>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="HandlingAgentDescription">
		<xs:annotation>
			<xs:documentation>Description of the handling agent.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="30"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="HandlingAgentIATACode">
		<xs:annotation>
			<xs:documentation>IATA assigned code defining a handling agent providing services for a particular flight.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="3"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="HandlingAgentService">
		<xs:annotation>
			<xs:documentation>Types of service that an agent may provide for a particular flight.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Aviation">
				<xs:annotation>
					<xs:documentation>Loading and unloading of passenger baggage</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="OnboardService">
				<xs:annotation>
					<xs:documentation>Passenger ticketing activities at check-in and gate</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Ground">
				<xs:annotation>
					<xs:documentation>Used when a single agent provides all generic handling services for a flight</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Freight">
				<xs:annotation>
					<xs:documentation>Loading and unloading of general cargo</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="PassengerService">
				<xs:annotation>
					<xs:documentation>Loading and unloading of mail</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="IATAFlightIdentifier">
		<xs:annotation>
			<xs:documentation>Flight identifier consisting of IATA carrier, flight number and an optional suffix. These components are typically concatenated into a single string when presented for display.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="CarrierIATACode" type="CarrierIATACode" nillable="false"/>
			<xs:element name="FlightNumber" type="FlightNumber" nillable="true"/>
			<xs:element name="FlightSuffix" type="FlightSuffix" nillable="true" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="IATAMasterFlightIdentifier">
		<xs:annotation>
			<xs:documentation>Flight identifier consisting of IATA carrier, flight number and an optional suffix. These components are typically concatenated into a single string when presented for display.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="CarrierIATACode" type="CarrierIATACode" nillable="true"/>
			<xs:element name="FlightNumber" type="FlightNumber" nillable="true"/>
			<xs:element name="FlightSuffix" type="FlightSuffix" nillable="true" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="IATALinkFlightIdentifier">
		<xs:annotation>
			<xs:documentation>Flight identifier consisting of IATA carrier, flight number and an optional suffix. These components are typically concatenated into a single string when presented for display.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="CarrierIATACode" type="CarrierIATACode" nillable="true"/>
			<xs:element name="FlightNumber" type="FlightNumber" nillable="true"/>
			<xs:element name="FlightSuffix" type="FlightSuffix" nillable="true" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ICAOFlightIdentifier">
		<xs:annotation>
			<xs:documentation>Flight identifier consisting of ICAO carrier, flight number and an optional suffix. These components are typically concatenated into a single string when presented for display.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="CarrierICAOCode" type="CarrierICAOCode" nillable="true"/>
			<xs:element name="FlightNumber" type="FlightNumber" nillable="true"/>
			<xs:element name="FlightSuffix" type="FlightSuffix" nillable="true" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="ImpairmentDescription">
		<xs:annotation>
			<xs:documentation>Reason for the impairment message. (N.B. Impairment might also be cleared.)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="1024"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ImpairmentEndDateTime">
		<xs:annotation>
			<xs:documentation>Time the impairment will be cleared.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:dateTime"/>
	</xs:simpleType>
	<xs:simpleType name="ImpairmentStartDateTime">
		<xs:annotation>
			<xs:documentation>Time the impairment will take affect.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:dateTime"/>
	</xs:simpleType>
	<xs:simpleType name="InfantPassengerCount">
		<xs:annotation>
			<xs:documentation>Total number of infant passengers on board.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer">
			<xs:minInclusive value="0"/>
			<xs:maxInclusive value="999"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="InformationLanguage">
		<xs:annotation>
			<xs:documentation>Language associated with the information.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Chinese"/>
			<xs:enumeration value="English"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="InformationRemark">
		<xs:annotation>
			<xs:documentation>A single piece of supplementary information of a particular type.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="InformationLanguage" type="InformationLanguage" nillable="true" minOccurs="0"/>
			<xs:element name="InformationText" type="InformationText" nillable="true" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="InformationText">
		<xs:annotation>
			<xs:documentation>Additional information in free format.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="512"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="InternationalPassengerCount">
		<xs:annotation>
			<xs:documentation>Number of passengers on board flying to/from international destinations.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer">
			<xs:minInclusive value="0"/>
			<xs:maxInclusive value="999"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="IrregularityAlphaIATACode">
		<xs:annotation>
			<xs:documentation>IATA assigned two character alphabetic code for expressing the reason for delays, diversions and other operational irregularities.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="2"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="IrregularityDetails">
		<xs:annotation>
			<xs:documentation>Complete detail of a single irregularity event including numeric and alphabetic codes and duration. Used to provide additional narrative on operational events such as delays and diversions to allow for corrective action to be taken and is not intended as a mechanism for apportioning blame.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="IrregularityAlphaIATACode" type="IrregularityAlphaIATACode" nillable="true" minOccurs="0"/>
			<xs:element name="IrregularityDuration" type="IrregularityDuration" nillable="true"/>
			<xs:element name="IrregularityNumericIATACode" type="IrregularityNumericIATACode" nillable="true" minOccurs="0"/>
			<xs:element name="IrregularityRole" type="IrregularityRole" nillable="false"/>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="IrregularityDuration">
		<xs:annotation>
			<xs:documentation>Duration of an irregularity assigned to an flight. This will typically be a value expressed in hours and minutes e.g. PT1H20M is 1 hour 20 minutes or minutes only as appropriate.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:duration"/>
	</xs:simpleType>
	<xs:simpleType name="IrregularityIATACode">
		<xs:annotation>
			<xs:documentation>IATA irregularity code assigned to a flight. This is a concatenation of both alphabetic and numeric codes.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="2"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="IrregularityNumericIATACode">
		<xs:annotation>
			<xs:documentation>IATA assigned two digit numeric code for expressing the reason for delays, diversions and other operational irregularities. Numeric codes are considered by IATA to be primary for the reporting of irregularity information.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="2"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="IrregularityRole">
		<xs:annotation>
			<xs:documentation>Identifies which irregularity code is transmitted (where multiple codes are defined within the system).</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Primary"/>
			<xs:enumeration value="Secondary"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="IsCompleteRecord">
		<xs:annotation>
			<xs:documentation>Boolean to indicate that a message contains all available data for a particular record. If false then the record contains only certain elements, typically indicating that data has been updated, but nothing can be assumed about the values of elements which are not included.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:boolean">
			<xs:pattern value="false|true"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="IsFlightClassificationDefault">
		<xs:annotation>
			<xs:documentation>Determines whether the flight classification is the default code for a flight service type (this relates to the BasicData FlightClassification message).</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:boolean">
			<xs:pattern value="false|true"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="IsGeneralAviationFlight">
		<xs:annotation>
			<xs:documentation>Boolean indicates that a flight is a general aviation flight (and not a regular/scheduled flight).</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:boolean">
			<xs:pattern value="false|true"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="IsReturnFlight">
		<xs:annotation>
			<xs:documentation>Boolean to indicate that a flight is a return flight.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:boolean">
			<xs:pattern value="false|true"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="IsTransferFlight">
		<xs:annotation>
			<xs:documentation>Boolean to indicate that a flight is a transfer flight.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:boolean">
			<xs:pattern value="false|true"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="HasVIPs">
		<xs:annotation>
			<xs:documentation>Boolean to indicate that one or more VIPs will be one the flight</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:boolean">
			<xs:pattern value="false|true"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="LastBagDateTime">
		<xs:annotation>
			<xs:documentation>Date and time of delivery of last bag into baggage carousel.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:dateTime"/>
	</xs:simpleType>
	<xs:simpleType name="LatestKnownDateTime">
		<xs:annotation>
			<xs:documentation>Latest known date and time of arrival or departure.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:dateTime"/>
	</xs:simpleType>
	<xs:simpleType name="LatestKnownDateTimeSource">
		<xs:annotation>
			<xs:documentation>The source of the latest known date and time of arrival or departure (Scheduled, Estimated Touchdown, Actual Blocks or Airborne).</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Actual"/>
			<xs:enumeration value="Estimated"/>
			<xs:enumeration value="Scheduled"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="LinkDay">
		<xs:annotation>
			<xs:documentation>Logical day of the corresponding linked flight (0-6: 0=today, 1=yesterday, etc.)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer">
			<xs:minInclusive value="0"/>
			<xs:maxInclusive value="6"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="LinkedFlight">
		<xs:annotation>
			<xs:documentation>Identification details for linked (by registration or scheduled turn) flight.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="IATALinkFlightIdentifier" type="IATALinkFlightIdentifier" nillable="false" minOccurs="0"/>
			<xs:element name="FlightIdentity" type="FlightIdentity" nillable="true"/>
			<xs:element name="FlightRepeatCount" type="FlightRepeatCount" nillable="true" minOccurs="0"/>
			<xs:element name="ScheduledDate" type="ScheduledDate" nillable="true"/>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="LinkedFlightID">
		<xs:annotation>
			<xs:documentation>Identification of the associated linked flight record. Note: a long data type is used to ensure that XML generation tools cater for very large integer values.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:long">
			<xs:maxInclusive value="999999999999999999"/>
			<xs:minInclusive value="0"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="LocalPassengerCount">
		<xs:annotation>
			<xs:documentation>Number of passengers on board flying to/from a local destination.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer">
			<xs:minInclusive value="0"/>
			<xs:maxInclusive value="999"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="LocationDescription">
		<xs:annotation>
			<xs:documentation>Description of the airport location.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="30"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="LocationIATACode">
		<xs:annotation>
			<xs:documentation>IATA assigned code for locations such as airports, bus, rail and ferry stations.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="3"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="LocationICAOCode">
		<xs:annotation>
			<xs:documentation>ICAO assigned code for locations such as airports, bus, rail and ferry stations.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="4"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="LocationName">
		<xs:annotation>
			<xs:documentation>Name of airport or city served by airport.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="30"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="MakeupPositionCapacity">
		<xs:annotation>
			<xs:documentation>Number of bags a make-up position can handle during a period in time.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer">
			<xs:minInclusive value="0"/>
			<xs:maxInclusive value="9999"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="MakeupPositionDescription">
		<xs:annotation>
			<xs:documentation>Description of the make-up position.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="30"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="MakeupPositionStatus">
		<xs:annotation>
			<xs:documentation>Operational status of the baggage make-up position (Open, Close, Unserviceable, Serviceable).</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="O"/>
			<xs:enumeration value="C"/>
			<xs:enumeration value="S"/>
			<xs:enumeration value="U"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="MakeupPositionStatusEndDateTime">
		<xs:annotation>
			<xs:documentation>The position's status end period.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:dateTime"/>
	</xs:simpleType>
	<xs:simpleType name="MakeupPositionStatusReason">
		<xs:annotation>
			<xs:documentation>Free text providing additional information regarding the status of the resource.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="AbstractTypeStatusReason"/>
	</xs:simpleType>
	<xs:simpleType name="MakeupPositionStatusStartDateTime">
		<xs:annotation>
			<xs:documentation>The position's status start period.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:dateTime"/>
	</xs:simpleType>
	<xs:complexType name="MasterFlight">
		<xs:annotation>
			<xs:documentation>Identification details for a related master flight.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="IATAMasterFlightIdentifier" type="IATAMasterFlightIdentifier" nillable="false" minOccurs="0"/>
			<xs:element name="FlightIdentity" type="FlightIdentity" nillable="true"/>
			<xs:element name="FlightRepeatCount" type="FlightRepeatCount" nillable="true" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="MasterFlightID">
		<xs:annotation>
			<xs:documentation>Identification of the associated master seasonal flight record. Note: a long data type is used to ensure that XML generation tools cater for very large integer values.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:long">
			<xs:maxInclusive value="999999999999999999"/>
			<xs:minInclusive value="0"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="MaximumPayload">
		<xs:annotation>
			<xs:documentation>Maximum payload that the aircraft can bear.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer">
			<xs:minInclusive value="0"/>
			<xs:maxInclusive value="9999999"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="NewFlightReason">
		<xs:annotation>
			<xs:documentation>Reason for newly added flight.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="3"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="NextInformationDateTime">
		<xs:annotation>
			<xs:documentation>Date and time of next information update for a delayed flight.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:dateTime"/>
	</xs:simpleType>
	<xs:simpleType name="NextStationActualDateTime">
		<xs:annotation>
			<xs:documentation>The actual date and time that an aircraft will arrive at its next port of call.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:dateTime"/>
	</xs:simpleType>
	<xs:simpleType name="NextStationEstimatedDateTime">
		<xs:annotation>
			<xs:documentation>The estimated date and time that an aircraft will arrive at its next port of call.</xs:documentation>
