<?xml version="1.0" encoding="UTF-8"?>
<!-- Copyright (c) 2009 Ultra Electronics Limited. All rights reserved. -->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://schema.ultra-as.com" xmlns="http://schema.ultra-as.com" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.0">
	<xs:include schemaLocation="Envelope.xsd"/>
	<xs:annotation>
		<xs:documentation>XML types used in messages for all IMB services.</xs:documentation>
	</xs:annotation>
	<xs:simpleType name="AbbreviatedChinese">
		<xs:annotation>
			<xs:documentation>Name of a location in a Chinese abbreviated form.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:simpleType name="AbstractTypeStatusReason">
		<xs:annotation>
			<xs:documentation>An Abstract Super Type that defines the general attributes of a StatusReason element.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:simpleType name="AccountCode">
		<xs:annotation>
			<xs:documentation>Flight account code; derived from the aircraft's registration for GA flights, or the airline IATA code for scheduled flights; cash payers will have a code of 000.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="3"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ActualOffBlocksDateTime">
		<xs:annotation>
			<xs:documentation>Actual date and time of departure from stand for an outbound aircraft.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:dateTime"/>
	</xs:simpleType>
	<xs:simpleType name="ActualOnBlocksDateTime">
		<xs:annotation>
			<xs:documentation>Actual date and time of arrival on stand for an inbound aircraft.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:dateTime"/>
	</xs:simpleType>
	<xs:simpleType name="AdultPassengerCount">
		<xs:annotation>
			<xs:documentation>Total number of adult passengers on board.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer">
			<xs:minInclusive value="0"/>
			<xs:maxInclusive value="999"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="AircraftCallsign">
		<xs:annotation>
			<xs:documentation>Callsign associated with the aircraft.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="10"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="AircraftCategoryIATACode">
		<xs:annotation>
			<xs:documentation>IATA aircraft type category (e.g. 3J = 3 jet engines, 4T = 4 turboprops, 2P = 2 propellers).</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="2"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="AircraftHeight">
		<xs:annotation>
			<xs:documentation>Height of the aircraft.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer">
			<xs:maxInclusive value="999"/>
			<xs:minInclusive value="0"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="AircraftLength">
		<xs:annotation>
			<xs:documentation>Length of the aircraft.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer">
			<xs:maxInclusive value="999"/>
			<xs:minInclusive value="0"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="AircraftMovementConfirmStatus">
		<xs:annotation>
			<xs:documentation>Status of the tow. C=tow automatically created and confirmed by user. U=tow automatically created and not confirmed by user.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="C"/>
			<xs:enumeration value="U"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="AircraftMovementDateTime">
		<xs:annotation>
			<xs:documentation>Actual or planned time an aircraft arrived/departed at/from the stand.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:dateTime"/>
	</xs:simpleType>
	<xs:simpleType name="AircraftMovementDirection">
		<xs:annotation>
			<xs:documentation>Tow is associated with the arrival or departure.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="FlightDirection"/>
	</xs:simpleType>
	<xs:simpleType name="AircraftMovementSequence">
		<xs:annotation>
			<xs:documentation>The tow number associated with an ordered tow sequence. Note: a long data type is used to ensure that XML generation tools cater for very large integer values.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:long">
			<xs:minInclusive value="0"/>
			<xs:maxInclusive value="999999999999999999"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="AircraftMovementTypeCode">
		<xs:annotation>
			<xs:documentation>Code representing the status of an on/off blocks aircraft movement.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="A">
				<xs:annotation>
					<xs:documentation>Archived but incomplete tow.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="C">
				<xs:annotation>
					<xs:documentation>Created on system initialisation to represent existing state when cutting over.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="O">
				<xs:annotation>
					<xs:documentation>Open tow. Allows tow off/on time to be set without having a complete parking sequence.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="R">
				<xs:annotation>
					<xs:documentation>Restricted tow.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="S">
				<xs:annotation>
					<xs:documentation>Slide back.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="T">
				<xs:annotation>
					<xs:documentation>Tow.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="AircraftNoiseLevel">
		<xs:annotation>
			<xs:documentation>Effective Perceived Noise Level (EPNL) of the aircraft.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="2"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="AircraftNoseNumber">
		<xs:annotation>
			<xs:documentation>Nose number of the aircraft.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="10"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="AircraftOperator">
		<xs:annotation>
			<xs:documentation>Airline that is operating the aircraft/flight.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="3"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="AircraftOwnerIATACode">
		<xs:annotation>
			<xs:documentation>IATA code of the aircraft owning airline.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="2"/>
			<xs:maxLength value="3"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="AircraftPassengerCapacity">
		<xs:annotation>
			<xs:documentation>Maximum number of passengers that an aircraft can accommodate.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer">
			<xs:minInclusive value="0"/>
			<xs:maxInclusive value="999"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="AircraftRegistration">
		<xs:annotation>
			<xs:documentation>Aircraft registration.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="10"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="AircraftRegistrationDescription">
		<xs:annotation>
			<xs:documentation>Description of the aircraft registration.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="30"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="AircraftSubtypeDescription">
		<xs:annotation>
			<xs:documentation>Description of the aircraft subtype.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="30"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="AircraftSubtypeIATACode">
		<xs:annotation>
			<xs:documentation>IATA assigned code for an aircraft subtype. </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="3"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="AircraftTailNumber">
		<xs:annotation>
			<xs:documentation>Tail number of the aircraft.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="10"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="AircraftTerminalCode">
		<xs:annotation>
			<xs:documentation>Terminal code for an aircraft, which may differ from the terminal presented to passengers.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="TerminalCode"/>
	</xs:simpleType>
	<xs:simpleType name="AircraftTypeIATACode">
		<xs:annotation>
			<xs:documentation>IATA assigned code for an aircraft type. </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="3"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="AircraftTypeICAOCode">
		<xs:annotation>
			<xs:documentation>ICAO assigned code for an aircraft type.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="4"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="AircraftWeight">
		<xs:annotation>
			<xs:documentation>Weight of the aircraft.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer">
			<xs:maxInclusive value="99999999"/>
			<xs:minInclusive value="0"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="AircraftWidth">
		<xs:annotation>
			<xs:documentation>Width of the aircraft.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer">
			<xs:maxInclusive value="999"/>
			<xs:minInclusive value="0"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="AirlineDescription">
		<xs:annotation>
			<xs:documentation>Description of the airline.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="30"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="AirlineIATACode">
		<xs:annotation>
			<xs:documentation>Two-character IATA-assigned code used to identify an airline</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="2"/>
			<xs:maxLength value="3"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="AirlineICAOCode">
		<xs:annotation>
			<xs:documentation>ICAO assigned code which uniquely defines an airline.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="3"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="AirlineName">
		<xs:annotation>
			<xs:documentation>Expanded name of the airline.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="30"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="AirlineTerminalCode">
		<xs:annotation>
			<xs:documentation>Terminal code associated with the flight's airline (the usual location associated with airline resources, which may differ from passenger or aircraft terminals).</xs:documentation>
		</xs:annotation>
		<xs:restriction base="TerminalCode"/>
	</xs:simpleType>
	<xs:simpleType name="AmendmentType">
		<xs:annotation>
			<xs:documentation>For Distribution services, this indicates the operation made to the data within the AODB (Create, Update, Delete).</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Create"/>
			<xs:enumeration value="Delete"/>
			<xs:enumeration value="Update"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="AnnouncementLanguage">
		<xs:annotation>
			<xs:documentation>The language that an announcement should be made in.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="30"/>
			<xs:enumeration value="Chinese"/>
			<xs:enumeration value="English"/>
			<xs:enumeration value="French"/>
			<xs:enumeration value="Korean"/>
			<xs:enumeration value="Japanese"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="AnnouncementParameter">
		<xs:annotation>
			<xs:documentation>Multi-purpose parameter used to store additional information (e.g. "Vouchers" for Long Delay announcements, or free format passenger names).</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="512"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="AnnouncementRepeatCount">
		<xs:annotation>
			<xs:documentation>How many times an announcement should be announced.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer">
			<xs:minInclusive value="1"/>
			<xs:maxInclusive value="9"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="AnnouncementType">
		<xs:annotation>
			<xs:documentation>Public address announcement type (e.g. Go To Gate, Boarding, Long Delay, Free Text).</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="30"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="AnnouncementZone">
		<xs:annotation>
			<xs:documentation>The area location that an announcement should be made.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="30"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="AssociatedGateNumber">
		<xs:annotation>
			<xs:documentation>Identity of a gate that has an association with another gate (e.g. a pair of gates can be determined to have a primary-secondary relationship; the associated gate is the secondary gate).</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="8"/>
			<xs:minLength value="1"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="BaggageMakeupCloseDateTime">
		<xs:annotation>
			<xs:documentation>Baggage make-up position close time</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:dateTime"/>
	</xs:simpleType>
	<xs:simpleType name="BaggageMakeupOpenDateTime">
		<xs:annotation>
			<xs:documentation>Baggage make-up position open time</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:dateTime"/>
	</xs:simpleType>
	<xs:complexType name="BaggageMakeupPosition">
		<xs:annotation>
			<xs:documentation>Full description of a flight's baggage make-up position allocation.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="BaggageMakeupCloseDateTime" type="BaggageMakeupCloseDateTime" nillable="true" minOccurs="0"/>
			<xs:element name="BaggageMakeupOpenDateTime" type="BaggageMakeupOpenDateTime" nillable="true" minOccurs="0"/>
			<xs:element name="BaggageMakeupPositionID" type="BaggageMakeupPositionID" nillable="true"/>
			<xs:element name="BaggageMakeupPositionRole" type="BaggageMakeupPositionRole" nillable="false"/>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="BaggageMakeupPositionID">
		<xs:annotation>
			<xs:documentation>Baggage make-up position identity.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="8"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="BaggageMakeupPositionRole">
		<xs:annotation>
			<xs:documentation>Baggage make-up position role - describes the purpose of each position when more than one is allocated to a single flight. Note: Current limit is two positions per flight</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Primary"/>
			<xs:enumeration value="Secondary"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="BaggageReclaimCarousel">
		<xs:annotation>
			<xs:documentation>Full description of a flight's baggage reclaim allocation.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="BaggageReclaimCarouselID" type="BaggageReclaimCarouselID" nillable="true"/>
			<xs:element name="BaggageReclaimCarouselRole" type="BaggageReclaimCarouselRole" nillable="false"/>
			<xs:element name="BaggageReclaimCloseDateTime" type="BaggageReclaimCloseDateTime" nillable="true" minOccurs="0"/>
			<xs:element name="BaggageReclaimOpenDateTime" type="BaggageReclaimOpenDateTime" nillable="true" minOccurs="0"/>
			<xs:element name="ExitDoorNumber" type="ExitDoorNumber" nillable="true" minOccurs="0"/>
			<xs:element name="FirstBagDateTime" type="FirstBagDateTime" nillable="true" minOccurs="0"/>
			<xs:element name="LastBagDateTime" type="LastBagDateTime" nillable="true" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="BaggageReclaimCarouselID">
		<xs:annotation>
			<xs:documentation>Baggage reclaim carousel identity.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="8"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="BaggageReclaimCarouselRole">
		<xs:annotation>
			<xs:documentation>Baggage reclaim carousel role - describes the purpose of each reclaim carousel when more than one is allocated to a single flight. Note: Current limit is two carousels per flight.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Primary"/>
			<xs:enumeration value="Secondary"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="BaggageReclaimCloseDateTime">
		<xs:annotation>
			<xs:documentation>Baggage reclaim carousel close time</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:dateTime"/>
	</xs:simpleType>
	<xs:simpleType name="BaggageReclaimOpenDateTime">
		<xs:annotation>
			<xs:documentation>Baggage reclaim carousel open time</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:dateTime"/>
	</xs:simpleType>
	<xs:simpleType name="BaggageTerminalCode">
		<xs:annotation>
			<xs:documentation>Terminal code associated with the baggage of a flight (check-in, make-up positions and carousels). This may differ from the other flight/aircraft terminals but is likely to be the same.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="TerminalCode"/>
	</xs:simpleType>
	<xs:complexType name="BasicData">
		<xs:annotation>
			<xs:documentation>Collection of reference data common to basic data messages.</xs:documentation>
		</xs:annotation>
		<xs:choice>
			<xs:element name="AircraftRegistration" nillable="false" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Identification" nillable="false">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="AircraftRegistration" type="AircraftRegistration" nillable="false"/>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="AircraftOwnerIATACode" type="AircraftOwnerIATACode" nillable="true" minOccurs="0"/>
						<xs:element name="AircraftPassengerCapacity" type="AircraftPassengerCapacity" nillable="true" minOccurs="0"/>
						<xs:element name="AircraftNoseNumber" type="AircraftNoseNumber" nillable="true" minOccurs="0"/>
						<xs:element name="AircraftSubtypeIATACode" type="AircraftSubtypeIATACode" nillable="true" minOccurs="0"/>
						<xs:element name="AircraftTailNumber" type="AircraftTailNumber" nillable="true" minOccurs="0"/>
						<xs:element name="AircraftRegistrationDescription" type="AircraftRegistrationDescription" nillable="true" minOccurs="0"/>
						<xs:element name="AirlineIATACode" type="AirlineIATACode" nillable="true" minOccurs="0"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="AircraftSubtype" nillable="false" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Identification" nillable="false">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="AircraftSubtypeIATACode" type="AircraftSubtypeIATACode" nillable="false"/>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="AircraftCategoryIATACode" type="AircraftCategoryIATACode" nillable="true" minOccurs="0"/>
						<xs:element name="AircraftHeight" type="AircraftHeight" nillable="true" minOccurs="0"/>
						<xs:element name="AircraftLength" type="AircraftLength" nillable="true" minOccurs="0"/>
						<xs:element name="AircraftNoiseLevel" type="AircraftNoiseLevel" nillable="true" minOccurs="0"/>
						<xs:element name="AircraftPassengerCapacity" type="AircraftPassengerCapacity" nillable="true" minOccurs="0"/>
						<xs:element name="AircraftSubtypeDescription" type="AircraftSubtypeDescription" nillable="true" minOccurs="0"/>
						<xs:element name="AircraftTypeIATACode" type="AircraftTypeIATACode" nillable="true" minOccurs="0"/>
						<xs:element name="AircraftTypeICAOCode" type="AircraftTypeICAOCode" nillable="true" minOccurs="0"/>
						<xs:element name="AircraftWeight" type="AircraftWeight" nillable="true" minOccurs="0"/>
						<xs:element name="AircraftWidth" type="AircraftWidth" nillable="true" minOccurs="0"/>
						<xs:element name="BusinessClassPassengerCapacity" type="BusinessClassPassengerCapacity" nillable="true" minOccurs="0"/>
						<xs:element name="EconomyClassPassengerCapacity" type="EconomyClassPassengerCapacity" nillable="true" minOccurs="0"/>
						<xs:element name="FirstClassPassengerCapacity" type="FirstClassPassengerCapacity" nillable="true" minOccurs="0"/>
						<xs:element name="FreightCapacity" type="FreightCapacity" nillable="true" minOccurs="0"/>
						<xs:element name="MaximumPayload" type="MaximumPayload" nillable="true" minOccurs="0"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Airline" nillable="false" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Identification" nillable="false">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="AirlineICAOCode" type="AirlineICAOCode" nillable="false"/>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="AirlineDescription" type="AirlineDescription" nillable="true" minOccurs="0"/>
						<xs:element name="AirlineIATACode" type="AirlineIATACode" nillable="false"/>
						<xs:element name="AirlineName" type="AirlineName" nillable="true" minOccurs="0"/>
						<xs:element name="FlightSectorCode" type="FlightSectorCode" nillable="true" minOccurs="0"/>
						<xs:element name="FlightServiceTypeIATACode" type="FlightServiceTypeIATACode" nillable="true" minOccurs="0"/>
