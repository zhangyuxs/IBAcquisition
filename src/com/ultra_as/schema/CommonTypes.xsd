			<xs:element name="TaskStatus" type="xs:string" minOccurs="0"/>
			<xs:element name="TaskLocation" type="xs:string" minOccurs="0"/>
			<xs:element name="Handler" type="xs:string" minOccurs="0"/>
			<xs:element name="PlannedStartTime" type="xs:dateTime"/>
			<xs:element name="PlannedEndTime" type="xs:dateTime" minOccurs="0"/>
			<xs:element name="EstimatedStartTime" type="xs:dateTime" minOccurs="0"/>
			<xs:element name="EstimatedEndTime" type="xs:dateTime" minOccurs="0"/>
			<xs:element name="ActualStartTime" type="xs:dateTime" minOccurs="0"/>
			<xs:element name="ActualEndTime" type="xs:dateTime" minOccurs="0"/>
			<xs:element name="AlertTime" type="xs:dateTime" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="VehicleUsage">
		<xs:annotation>
			<xs:documentation>The details of one vehicle used to service a flight.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="RecordIdentification" type="RecordIdentification"/>
			<xs:element name="VehicleType" type="VehicleType" nillable="false"/>
			<xs:element name="VehicleID" type="VehicleID" nillable="false"/>
			<xs:element name="Repeat" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicator that this record is for the n'th time the vehicle was used for the flight</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:minInclusive value="0"/>
						<xs:maxInclusive value="99"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="StartTime" type="xs:dateTime">
				<xs:annotation>
					<xs:documentation>Date and time the vehicle usage started</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EndTime" type="xs:dateTime" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Date and time the vehilce usage ended</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SubType" type="SubType" nillable="true" minOccurs="0"/>
			<xs:element name="Count" type="Count" nillable="true" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="VehicleType">
		<xs:annotation>
			<xs:documentation>Type of vehicle the record is for, e.g. Bus, Steps, Tug</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="5"></xs:maxLength>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="VehicleID">
		<xs:annotation>
			<xs:documentation>Unique identification for the vehicle, e.g. Registration</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="20"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SubType">
      <xs:annotation>
         <xs:documentation>Additional classification within Vehicle or resource type</xs:documentation>
      </xs:annotation>
      <xs:restriction base="xs:string">
         <xs:maxLength value="38"></xs:maxLength>
      </xs:restriction>
   </xs:simpleType>
   <xs:simpleType name="Count">
      <xs:annotation>
         <xs:documentation>Used count for resources usage</xs:documentation>
      </xs:annotation>
      <xs:restriction base="xs:integer">
         <xs:minInclusive value="0"/>
         <xs:maxInclusive value="999"/>
      </xs:restriction>
   </xs:simpleType>
	<xs:complexType name="ExtendedFlightData">
		<xs:annotation>
			<xs:documentation>Collection of extended flight data common to extended flight data messages.</xs:documentation>
		</xs:annotation>
		<xs:choice>
			<xs:element name="VIPData" type="VIPData" nillable="false" minOccurs="0"/>
			<xs:element name="FlightTask" type="FlightTask" nillable="false" minOccurs="0"/>
			<xs:element name="VehicleUsage" type="VehicleUsage" nillable="false" minOccurs="0"/>
		</xs:choice>
	</xs:complexType>
	<xs:simpleType name="VIPName">
		<xs:annotation>
			<xs:documentation>Name of the VIP boarding the Flight</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="100"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="VIPLevel">
		<xs:annotation>
			<xs:documentation>Importance of the VIP on the Flight</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="VVIP"/>
			<xs:enumeration value="VIP"/>
			<xs:enumeration value="CIP"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Title">
		<xs:annotation>
			<xs:documentation>Title of the VIP on the Flight</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="100"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="TelephoneNumber">
		<xs:annotation>
			<xs:documentation>Title of the VIP on the Flight</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="100"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Gender">
		<xs:annotation>
			<xs:documentation>Gender of the VIP on the Flight</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Male"/>
			<xs:enumeration value="Female"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Description">
		<xs:annotation>
			<xs:documentation>Description of the VIP / Journey on the Flight</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="1000"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="NumberOfRetinues">
		<xs:annotation>
			<xs:documentation>Number of retinues accompanying the VIP on the Flight</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer">
			<xs:minInclusive value="0"/>
			<xs:maxInclusive value="999"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="NumberOfBags">
		<xs:annotation>
			<xs:documentation>Number of bags accompanying the VIP on the Flight</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer">
			<xs:minInclusive value="0"/>
			<xs:maxInclusive value="999"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="AircraftVisitSequence">
        <xs:annotation>
            <xs:documentation>The tow number associated with an ordered tow sequence. Note: a long data type is used to ensure that XML generation tools cater for very large integer values.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:long">
            <xs:minInclusive value="0"/>
            <xs:maxInclusive value="999999999999999999"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:complexType name="AircraftVisitIdentification">
        <xs:sequence>
            <xs:element name="AircraftVisitSequence" type="AircraftVisitSequence" nillable="false" maxOccurs="1"/>
            <xs:element name="AircraftRegistration" type="AircraftRegistration" nillable="false" maxOccurs="1"/>
            <xs:element name="ScheduledDate" type="ScheduledDate" nillable="false" maxOccurs="1"/>
        </xs:sequence>
    </xs:complexType>
	<xs:simpleType name="BoardingAirportIATACode">
        <xs:annotation>
            <xs:documentation>IATA code identifying the airport a VIP boarded at</xs:documentation>
        </xs:annotation>
        <xs:restriction base="LocationIATACode"/>
    </xs:simpleType>
    <xs:simpleType name="DepartingAirportIATACode">
        <xs:annotation>
            <xs:documentation>IATA code identifying the airport a VIP will depart at</xs:documentation>
        </xs:annotation>
        <xs:restriction base="LocationIATACode"/>
    </xs:simpleType>
    <xs:simpleType name="VehicleRegistrationNumber">
        <xs:annotation>
            <xs:documentation>Vehicle registration(s) associated with the VIP</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
            <xs:maxLength value="500"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="RoomUsed">
        <xs:annotation>
            <xs:documentation>Identifies any private rooms used by the VIP</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
            <xs:maxLength value="20"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="HallUsed">
        <xs:annotation>
            <xs:documentation>Identifies the hall (or zone) the VIP used</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
            <xs:maxLength value="20"/>
        </xs:restriction>
    </xs:simpleType>
	<xs:complexType name="RecordIdentification">
		<xs:annotation>
			<xs:documentation>Name of originating system and its sequence number to uniquely identify the record</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="OriginatingSystem">
				<xs:annotation>
					<xs:documentation>Originating system of the record</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="20"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="OriginatingID">
				<xs:annotation>
					<xs:documentation>Original system's unique sequence number for the record</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:long">
						<xs:maxInclusive value="999999999999999999"/>
						<xs:minInclusive value="0"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="WeatherDataCategory">
		<xs:annotation>
			<xs:documentation>Basic data category that can be subscribed to. (N.B. The "All" category is special for service provider processing only, it cannot be set by client systems.)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="All"/>
			<xs:enumeration value="Observations"/>
			<xs:enumeration value="Forecasts"/>
		</xs:restriction>
	</xs:simpleType>	
</xs:schema>
